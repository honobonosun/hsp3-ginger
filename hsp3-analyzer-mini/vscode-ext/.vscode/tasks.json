{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "ham:build",
            "type": "shell",
            "command": "cargo",
            "args": [
                "build"
            ],
            "options": {
                "cwd": ".."
            },
            "group": "build"
        },
        {
            "label": "ext:build",
            "type": "shell",
            "command": "yarn",
            "args": [
                "build"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": "ham:build"
        }
    ]
}