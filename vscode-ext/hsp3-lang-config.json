{
  "comments": {
    "lineComment": "//",
    "blockComment": [
      "/*",
      "*/"
    ]
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    [
      "{\"",
      "\"}"
    ],
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    {
      "open": "'",
      "close": "'",
      "notIn": [
        "string",
        "comment"
      ]
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": [
        "string",
        "comment"
      ]
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "'",
      "'"
    ]
  ],
  "wordPattern": "[a-zA-Z_][a-zA-Z0-0_]+"
}
